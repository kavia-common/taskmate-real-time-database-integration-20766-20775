# Environment variables required by the React frontend (Create React App).
# Copy this file to `.env` and fill in the values. Do NOT commit your actual `.env`.
# These values are injected at build time and must be prefixed with REACT_APP_.

# Your Supabase project URL (from the Supabase dashboard -> Project Settings -> API)
REACT_APP_SUPABASE_URL=https://YOUR-PROJECT-REF.supabase.co

# Your Supabase anon public API key (from the Supabase dashboard -> Project Settings -> API)
REACT_APP_SUPABASE_KEY=YOUR_PUBLIC_ANON_KEY
